    "cookie-parser": "latest",
    "debug": "latest",
    "ejs": "latest",
    "express": "latest",
    "express-session": "latest",
    "mkdirp": "latest",
    "morgan": "latest",
    "js-yaml": "latest",
    "underscore": "latest"
  }
}
